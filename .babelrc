{
   "presets": [
      [
         "@babel/preset-env",
         {
            "bugfixes": true
         }
      ],
      "@babel/preset-react"
   ],
   "plugins": [
      "@loadable/babel-plugin",
      "babel-plugin-styled-components",
      [
         "@babel/plugin-proposal-class-properties",
         {
            "loose": false
         }
      ]
   ],
   "ignore": [
      "node_modules",
      "build"
   ]
}