{
  "name": "1_start_with_ssr",
  "version": "1.0.0",
  "description": "",
  "main": "build/server.js",
  "scripts": {
    "start": "rm -rf build && webpack --mode=development --config webpack/server/webpack.server.development.js --config webpack/client/webpack.client.development.js && node build/server.js",
    "startWatch": "npm-run-all --parallel startWatch:*",
    "startWatch:build-server": "webpack --config webpack/server/webpack.server.development.js --watch",
    "startWatch:build-client": "webpack --config webpack/client/webpack.client.development.js --watch --no-cache",
    "startWatch:server": "nodemon --watch build --exec node build/server.js",
    "build:prod": "rm -rf ./build && webpack --config webpack/server/webpack.server.production.js --config webpack/client/webpack.client.production.js",
    "build:dev": "rm -rf ./build && webpack --config webpack/dev/webpack.dev.server.js --config webpack/dev/webpack.dev.client.js",
    "build:serve":"npm run build:prod && node build/server"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@loadable/component": "^5.15.2",
    "@loadable/server": "^5.15.2",
    "compression": "^1.7.4",
    "express": "^4.18.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet-async": "^1.3.0",
    "react-router-dom": "^6.4.5",
    "styled-components": "^5.3.5"
  },
  "devDependencies": {
    "@babel/core": "^7.20.5",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@loadable/babel-plugin": "^5.13.2",
    "@loadable/webpack-plugin": "^5.15.2",
    "@swc/core": "^1.3.23",
    "babel-loader": "^9.1.0",
    "babel-plugin-styled-components": "^2.0.7",
    "css-loader": "^6.7.1",
    "css-minimizer-webpack-plugin": "^4.2.2",
    "dotenv": "^16.0.3",
    "dotenv-webpack": "^8.0.1",
    "mini-css-extract-plugin": "^2.7.2",
    "nodemon": "^2.0.20",
    "npm-run-all": "^4.1.5",
    "optimize-css-assets-webpack-plugin": "^6.0.1",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-middleware": "^6.0.1",
    "webpack-devserver": "0.0.6",
    "webpack-hot-middleware": "^2.25.3",
    "webpack-merge": "^5.8.0",
    "webpack-node-externals": "^3.0.0"
  }
}
